<%- include('partials/header') -%>

<h1>Home</h1>
<p><%= homeStartString %></p>

<% posts.forEach(function (post){ %>

    <h1> <%= post.PostTitle %></h1>
    <p>  <%= post.PostData.substring(0,Math.min(100,post.PostData.length)) + "..." %>
         <a href= "/posts/<%= post.PostTitle %>" >Read More </a>
    </p>
<% }); %>

<%- include('partials/footer') -%>
